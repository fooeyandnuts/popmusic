<div class="container"></div><div class="container-fluid"></div><div class="container"><br>
	<div class="row-fluid">
	  <div class="span3 block block-sm">
			<h1>Saved Courses</h1>
			<ul>
				<% @courses.each do |c| %>
					<li>
						<%= link_to c.name, c %>
						<%= link_to "Edit", edit_course_path(c) %>
						<%= link_to "Delete Course", course_path(c), method: :delete %>
					</li>
				<% end %>
					<li><%= link_to "New Course", new_course_path %></li>
			</ul>
		</div>
	</div>
</div>
